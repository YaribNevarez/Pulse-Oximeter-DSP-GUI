#ifndef SRC_POXI_H_
#define SRC_POXI_H_
#include "xil_types.h"

typedef struct
{
	uint8_t (* initialize)(void);
	uint8_t (* run)(void);
	uint8_t (* disposal)(void);
} Poxi;

Poxi * Poxi_instance(vois);
#endif /* SRC_POXI_H_ */
